{
    "authors": [
        {
            "email": "minottoemanuele@gmail.com",
            "name": "Emanuele Minotto"
        }
    ],
    "autoload": {
        "psr-4": {
            "EmanueleMinotto\\SafeBrowsingBundle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EmanueleMinotto\\SafeBrowsingBundle\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "description": "Symfony Bundle for Google Safe Browsing",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "keywords": [
        "browsing",
        "bundle",
        "google",
        "safe"
    ],
    "license": "MIT",
    "name": "emanueleminotto/safe-browsing-bundle",
    "require": {
        "php": "^5.5|^7.0",
        "doctrine/cache": "^1.6",
        "guzzlehttp/guzzle": "^6.1",
        "symfony/framework-bundle": "^2.8|^3.0",
        "symfony/validator": "^2.8|^3.0"
    },
    "require-dev": {
        "matthiasnoback/symfony-dependency-injection-test": "^0.7.6",
        "phpunit/phpunit": "^4.0|^5.0",
        "scrutinizer/ocular": "^1.3",
        "squizlabs/php_codesniffer": "^2.5"
    },
    "scripts": {
        "test": "phpunit"
    },
    "type": "symfony-bundle"
}
